{
  "name": "falseclock/dbd-php",
  "type": "library",
  "version": "3.0.0",
  "description": "Database driver for PHP (PostgreSQL, MySQL, MSSQL, OData, YellowERP, 1ะก)",
  "keywords": [
    "pdo",
    "dbd",
    "dbi",
    "sql",
    "cache",
    "driver",
    "postgre",
    "PostgreSQL",
    "MySQL",
    "MSSQL",
    "Microsoft SQL Server",
    "OData",
    "1ะก",
    "1C",
    "YellowERP",
    "Yellow ERP"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Nurlan Mukhanov",
      "email": "nurike@gmail.com"
    }
  ],
  "require": {
    "php": ">=8.0",
    "psr/simple-cache": "^1.0.1",
    "openlss/lib-array2xml": "^1.0.0",
    "falseclock/dbd-php-entity": "^3.0.0",
    "ext-json": "*",
    "ext-simplexml": "*",
    "ext-dom": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5.20",
    "php-coveralls/php-coveralls": "^2.5.2"
  },
  "suggest": {
    "ext-memcache": "Required to support Memcached",
    "ext-pgsql": "Required to support PostgreSQL database",
    "ext-curl": "Required to support OData requests"
  },
  "autoload": {
    "psr-4": {
      "DBD\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "DBD\\Tests\\": "tests/"
    }
  }
}
